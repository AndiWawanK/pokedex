{
    "plugins": [
      [
        "module-resolver",
        {
          "cwd": "babelrc",
          "root": ["./src"],
          "extensions": [".js", ".ios.js", ".android.js"],
          "alias": {
            "assets": "./src/assets",
            "components": "./src/components",
            "constants": "./src/constants",
            "navigation": "./src/navigation",
            "scenes": "./src/scenes",
            "styles": "./src/styles",
            "utils": "./src/utils"
          }
        }
      ]
    ]
  }